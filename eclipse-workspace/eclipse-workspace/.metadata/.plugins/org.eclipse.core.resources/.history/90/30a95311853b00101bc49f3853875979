package chap13;

import java.util.ArrayList;

public class Example {

	public static void main(String[] args) {
		ArrayList <String> cats =new ArrayList<String>();
		cats.add("페르시안");
		cats.add(null);
		System.out.println(cats);
		cats.add(1,"샴");
		System.out.println(cats);
		cats.add(2,"러시안블루");
		System.out.println(cats);
		

	}

}
